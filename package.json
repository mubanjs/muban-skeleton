{
  "name": "muban-skeleton",
  "description": "A skeleton with full dev and build setup to get up and running quickly",
  "version": "1.0.0",
  "author": "Arjan van Wijk",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --ext ts --exec 'ts-node' scripts/devserver/devserver.ts --watch scripts/devserver/devserver.ts --watch scripts/devserver/getServerBundle.ts",
    "build": "cross-env-shell NODE_ENV=production && yarn clean && yarn generate:code",
    "build:debug": "cross-env-shell NODE_ENV=development && yarn clean && yarn generate:code",
    "build:preview": "yarn clean && npm-run-all generate:*",
    "generate:code": "cross-env TS_NODE_PROJECT=\"config/tsconfig.webpack.json\" webpack -c ./config/webpack.config.ts",
    "generate:templates": "ts-node scripts/generateTemplates.ts",
    "generate:mock-server": "ts-node scripts/mocks/generateMockPackageJson.ts",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o ./dist/storybook",
    "preview": "ts-node scripts/preview-server.ts",
    "analyze": "webpack-bundle-analyzer dist/bundlesize-profile.json ./dist/site",
    "prettify": "prettier --write \"src/**/*.{js,ts,scss,json}\"",
    "clean": "shx rm -rf ./dist",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "deploy": "cross-env rsync -avPx dist/node/* narie-express-test@eu.dev.monkapps.com:~/node/ --delete-after"
  },
  "dependencies": {
    "@muban/muban": "^1.0.0-alpha.13",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@mediamonks/monck": "^1.0.0-alpha.3",
    "@muban/storybook": "^7.0.0-alpha.6",
    "@storybook/preset-scss": "^1.0.3",
    "@types/fs-extra": "^9.0.4",
    "@webpack-cli/init": "^1.0.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "detect-port": "^1.3.0",
    "express": "^4.17.1",
    "faker": "^5.1.0",
    "fs-extra": "^9.0.1",
    "memory-fs": "^0.5.0",
    "mini-css-extract-plugin": "^1.3.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "opn": "^6.0.0",
    "pem": "^1.14.4",
    "prettier": "^2.2.1",
    "recursive-readdir": "^2.2.2",
    "require-from-string": "^2.0.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "serve-index": "^1.9.1",
    "shelljs": "^0.8.4",
    "shx": "^0.3.3",
    "source-map-loader": "^1.1.2",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2",
    "vuepress": "^1.7.1",
    "webpack": "^5.9.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-middleware": "^4.0.2"
  }
}
