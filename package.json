{
  "name": "muban-skeleton",
  "version": "1.0.0-alpha.1",
  "type": "module",
  "files": [
    "src",
    ".editorconfig",
    ".pota",
    ".storybook",
    "static",
    "patches"
  ],
  "scripts": {
    "build": "pota build --output=dist/site",
    "storybook": "npm run apply-storybook-patches ; start-storybook -p 6006",
    "storybook:build": "npm run apply-storybook-patches ; build-storybook -o ./dist/storybook",
    "apply-storybook-patches": "patch --forward node_modules/@storybook/builder-webpack5/dist/cjs/preview/iframe-webpack.config.js < patches/cjs-entry.patch ; patch --forward node_modules/@storybook/core-server/dist/cjs/build-dev.js < patches/name-check.patch ; patch --forward node_modules/@muban/storybook/dist/client/preview/render.js < patches/story-context.patch",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "./deploy-docs.sh"
  },
  "dependencies": {
    "@mediamonks/monck": "^1.0.0-alpha.3",
    "@muban/storybook": "^7.0.0-alpha.14",
    "@storybook/builder-webpack5": "^6.4.0",
    "@storybook/manager-webpack5": "^6.4.0",
    "require-from-string": "^2.0.2"
  },
  "peerDependencies": {
    "@mediamonks/eslint-config-base": "^2.0.0-alpha.0",
    "@pota/cli": "^1.0.0-rc.6",
    "@pota/webpack-skeleton": "^1.0.0-rc.33",
    "@muban/muban": "1.0.0-alpha.26",
    "@muban/template": "^1.0.0-alpha.1",
    "@vue/reactivity": "^3.2.23",
    "classnames": "^2.3.1",
    "seng-scss": "^1.2.5",
    "normalize.css": "^8.0.1"
  }
}
