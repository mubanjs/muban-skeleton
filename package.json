{
  "name": "muban-skeleton",
  "description": "A skeleton with full dev and build setup to get up and running quickly",
  "version": "1.0.0",
  "author": "Arjan van Wijk",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --ext ts --exec 'ts-node' scripts/devserver/devserver.ts --watch scripts/devserver/devserver.ts --watch scripts/devserver/getServerBundle.ts",
    "watch": "NODE_ENV=development nodemon --ext ts --exec 'ts-node' watch.ts --watch ./watch.ts",
    "build": "yarn clean && cross-env-shell NODE_ENV=production yarn generate:code",
    "build:debug": "yarn clean && cross-env-shell NODE_ENV=development yarn generate:code",
    "build:preview": "yarn clean && cross-env-shell NODE_ENV=production MUBAN_PREVIEW=true npm-run-all generate:*",
    "generate:code": "cross-env TS_NODE_PROJECT=\"config/tsconfig.webpack.json\" webpack -c ./config/webpack.config.ts",
    "generate:templates": "ts-node scripts/generateTemplates.ts",
    "generate:mock-server": "ts-node scripts/mocks/generateMockPackageJson.ts",
    "storybook": "start-storybook -p 6006 -s ./src/public,./src/pages",
    "storybook:build": "build-storybook -o ./dist/storybook -s ./src/public,./src/pages",
    "preview": "ts-node scripts/preview-server.ts",
    "analyze": "webpack-bundle-analyzer dist/bundlesize-profile.json ./dist/site",
    "check-types": "tsc --project tsconfig.json --noEmit --noUnusedLocals",
    "prettify": "prettier --write \"src/**/*.{js,ts,scss,json}\"",
    "clean": "shx rm -rf ./dist",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "./deploy-docs.sh",
    "deploy": "cross-env rsync -avPx dist/node/* narie-express-test@eu.dev.monkapps.com:~/node/ --delete-after"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@muban/muban": "^1.0.0-alpha.19",
    "core-js": "^3.9.1",
    "normalize.css": "^8.0.1",
    "seng-scss": "^1.2.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/helpers": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.13",
    "@babel/plugin-proposal-function-sent": "^7.12.13",
    "@babel/plugin-proposal-json-strings": "^7.13.8",
    "@babel/plugin-proposal-numeric-separator": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-proposal-throw-expressions": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-react-display-name": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-stage-3": "^7.8.3",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.8",
    "@mediamonks/monck": "^1.0.0-alpha.3",
    "@muban/storybook": "^7.0.0-alpha.11",
    "@storybook/preset-scss": "^1.0.3",
    "@types/fs-extra": "^9.0.4",
    "@webpack-cli/init": "^1.0.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "copy-webpack-plugin": "^8.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "detect-port": "^1.3.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "faker": "^5.1.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.0.1",
    "get-port": "^5.1.1",
    "memory-fs": "^0.5.0",
    "mini-css-extract-plugin": "^1.3.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "opn": "^6.0.0",
    "pem": "^1.14.4",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss": "^8.2.8",
    "postcss-flexbugs-fixes": "^5.0.2",
